{
  "name": "@undb/sqlite",
  "description": "",
  "version": "0.0.0",
  "dependencies": {
    "@mikro-orm/better-sqlite": "^5.7.4",
    "@mikro-orm/core": "^5.7.4",
    "@mikro-orm/migrations": "^5.7.4",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@undb/core": "^0.0.0",
    "@undb/domain": "^0.0.0",
    "date-fns": "^2.30.0",
    "lodash-es": "^4.17.21",
    "oxide.ts": "^1.1.0",
    "reflect-metadata": "^0.1.13",
    "ts-mixer": "^6.0.3"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.7.4",
    "@mikro-orm/reflection": "^5.7.4",
    "@swc/core": "^1.3.56",
    "@types/lodash-es": "^4.17.7",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsconfig": "^0.0.0",
    "tslib": "^2.5.0",
    "tsup": "^6.7.0",
    "type-fest": "^3.9.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0",
    "vitest-mock-extended": "^1.1.3"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.js"
  },
  "main": "./dist/index",
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts"
    ]
  },
  "private": true,
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "coverage": "vitest run --coverage",
    "db:migration:create": "NODE_OPTIONS=\"--loader ts-node/esm\" mikro-orm migration:create",
    "db:schema:fresh": "NODE_OPTIONS=\"--loader ts-node/esm\" mikro-orm schema:fresh -r",
    "dev": "tsup --watch",
    "prebuild": "pnpm run clean",
    "test": "TZ=UTC NODE_OPTIONS=\"--loader ts-node/esm\" vitest run",
    "test:watch": "TZ=UTC NODE_OPTIONS=\"--loader ts-node/esm\" vitest"
  },
  "type": "module"
}
