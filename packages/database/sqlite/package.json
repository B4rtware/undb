{
  "name": "@egodb/sqlite",
  "description": "",
  "version": "0.0.0",
  "dependencies": {
    "@egodb/core": "^0.0.0",
    "@egodb/domain": "^0.0.0",
    "@mikro-orm/better-sqlite": "^5.6.14",
    "@mikro-orm/core": "^5.6.14",
    "@mikro-orm/migrations": "^5.6.14",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "date-fns": "^2.29.3",
    "lodash-es": "^4.17.21",
    "oxide.ts": "^1.1.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.6.14",
    "@mikro-orm/reflection": "^5.6.14",
    "@swc/core": "^1.3.40",
    "@types/lodash-es": "^4.17.6",
    "rimraf": "^4.4.0",
    "ts-node": "^10.9.1",
    "tsconfig": "^0.0.0",
    "tslib": "^2.5.0",
    "tsup": "^6.6.3",
    "type-fest": "^3.6.1",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2",
    "vitest-mock-extended": "^1.1.0"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.js"
  },
  "main": "./dist/index",
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts"
    ]
  },
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "coverage": "vitest run --coverage",
    "db:migration:create": "NODE_OPTIONS=\"--loader ts-node/esm\" mikro-orm migration:create --initial",
    "db:schema:fresh": "NODE_OPTIONS=\"--loader ts-node/esm\" mikro-orm schema:fresh -r",
    "dev": "tsup --watch",
    "prebuild": "pnpm run clean",
    "test": "TZ=UTC NODE_OPTIONS=\"--loader ts-node/esm\" vitest run",
    "test:watch": "TZ=UTC NODE_OPTIONS=\"--loader ts-node/esm\" vitest"
  },
  "type": "module"
}
