{
  "extends": "tsconfig/nextjs.json",
  "include": [
    "next-env.d.ts",
    "emotion.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "experimentalDecorators": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
