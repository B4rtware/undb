{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "cache": false,
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "test:watch": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false
    },
    "db:schema:fresh": {
      "cache": false
    },
    "db:migration:up": {
      "cache": false
    }
  }
}
